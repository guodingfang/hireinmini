/* pages/product_derails/product_derails.wxss */
.pro-top{
    width: 100%;
    height: 320rpx;
}
.pro-top image{
    width: 100%;
    height: 100%;
}
.pro-title{
    padding: 30rpx 25rpx;
    font-size: 32rpx;
    background: #fff;
}
.pro-title view{
    display: inline-block;
}
.pro-fen{
    color:#fea201;
    text-align: right;
}
.pro-fen image{
    width: 30rpx;
    height: 30rpx;
    margin-left: 15rpx;
    vertical-align: -3rpx;
}
.pro-detail{
    padding:0 25rpx;
    background: #fff;
    margin-top: 15rpx;
    overflow: hidden;
}
.pro-des{
    padding: 25rpx 0;
    border-bottom: 3rpx solid #f0f0f0;
    font-size: 32rpx;
}
.pro-des image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
    vertical-align: -3rpx;
}
.pro-content{
    line-height: 40rpx;
    font-size: 30rpx;
    padding: 20rpx 0;
    margin-bottom: 20rpx;
}
.com-ment {
    display: inline-block;
    font-size: 32rpx;
    border:none;
    color: #fea201;
    text-align: center;
    overflow: visible;
    background: #fff;
    line-height: 50rpx;
}
button:after{
    border: none;
}
.returnindex {
    width: 76rpx;
    height: 76rpx;
    background: rgba(0, 0, 0, 0.7);
    top: 64%;
    right: 20rpx;
    border-radius: 50rpx;
    position: fixed;
    z-index:200;
}

.returnindexpic {
    width: 38rpx;
    height: 38rpx;
    margin-left: 19rpx;
    margin-top: 19rpx;
}