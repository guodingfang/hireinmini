
.status-list {
  display: flex;
  position: fixed;
  top: 80rpx;
  align-items: center;
  background-color: #ffffff;
  width: 100%;
  height: 80rpx;
  padding: 0 30rpx;
}

.status-item {
  padding: 5rpx 20rpx;
  background-color: #F5F5F5;
  border-radius: 20rpx;
  color: #666666;
  font-size: 28rpx;
}

.status-item + .status-item {
  margin-left: 20rpx;
}

.not-text {
  color: #666666;
  font-size: 30rpx;
  margin: 24rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
}