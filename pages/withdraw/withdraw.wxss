.model {
  width: 690rpx;
  background: #FFFFFF;
  border-radius: 30rpx;
  margin: 20rpx 30rpx;
  overflow: hidden;
}

.bank-card {
  width: 630rpx;
  height: 90rpx;
  border: 1rpx solid #E5E5E5;
  border-radius: 20rpx;
  margin: 30rpx;
  display: flex;
  align-items: center;
}

.bank-card-input {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.pl-withdraw-input,
.pl-bank-card-input {
  font-size: 28rpx;
  color: #999999;
  font-weight: 500;
}


.withdraw {
  margin: 10rpx 30rpx 0 30rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #E5E5E5;
}

.sign {
  width: 50rpx;
  font-size: 50rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 50rpx;
  color: #1A1A1A;
}

.withdraw-input {
  flex: 1;
  padding-left: 10rpx;
  font-size: 50rpx;
  height: 60rpx;
  font-weight: bold;
  color: #1A1A1A;
}

.icon-close {
  width: 30rpx;
  height: 30rpx;
  padding: 30rpx;
  margin-right: -30rpx;
}

.price-total {
  height: 80rpx;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
  font-size: 22rpx;
  color: #666666;
  box-sizing: border-box;
  font-family: PingFang SC;
}

.btn-all {
  color: #0400E6;
  height: 100%;
  display: flex;
  align-items: center;
}

.withdraw-complete {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.withdraw-complete-img {
  width: 256rpx;
  height: 204rpx;
  margin: 147rpx 247rpx 4rpx 247rpx;
}

.withdraw-complete-text {
  text-align: center;
  width: 400rpx;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 36rpx;
  color: #666666;
  margin-bottom: 60rpx;
}

.withdraw-btn {
  width: 640rpx;
  height: 80rpx;
  background: #0400E6;
  border-radius: 20rpx;
  margin: 30rpx auto;
  font-weight: bold;
  color: #FFFFFF;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}