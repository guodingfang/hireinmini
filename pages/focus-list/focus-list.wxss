Page {
  background-color: #ffffff;
}

.item {
  display: flex;
  height: 120rpx;
  align-items: center;
  border-bottom: 0.5rpx solid #e5e5ee;
  padding: 20rpx;
  box-sizing: border-box;
}

.user-logo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.info {
  margin-right: auto;
}

.name {
  margin-left: 24rpx;
  font-size: 24rpx;
  color: #1a1a1a;
}

.btn {
  width: 130rpx;
  height: 50rpx;
  background-color: #FFEED9;
  color: #FFA12C;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border-radius: 12rpx;
}

.not-more {
  height: 100rpx;
  font-size: 20rpx;
  color: #999999;
  display: flex;
  align-items: center;
  justify-content: center;
}