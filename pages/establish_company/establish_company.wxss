/* pages/establish_company/establish_company.wxss */
@import "../../common/css/list.wxss";

.work-list {
    padding: 20rpx 0 6rpx 0;
}

.works {
    margin-right: 10rpx;
    display: inline-block;
}

.work {
    padding: 0 10rpx;
    display: inline-block;
    text-align: center;
    border: 1rpx solid #ccc;
    border-radius: 6rpx;
    font-size: 26rpx;
    color: #ccc;
}

.work-click {
    border: 1rpx solid #2ab7ff;
    color: #2ab7ff;
}

/* 遮罩 */

.shade {
    position: fixed;
    top: 0;
    right: 0;
    z-index: 999998;
    background: #000;
    width: 100%;
    height: 100%;
    filter: alpha(Opacity=50);
    -moz-opacity: 0.5;
    opacity: 0.5;
}
.gps{
    position: absolute;
    right: 0rpx;
    top: 10rpx;
    width: 155rpx;
    height: 70rpx;
    background: #fff;
    z-index: 2;
    border:3rpx solid #e0e0e0;
    border-radius: 100rpx;
    
}
.gps image{
    width: 70rpx;
    height: 70rpx;
}
.gps view{
    vertical-align: top;
    display: inline-block;
    line-height: 65rpx;
    margin-right: 5rpx;
    margin-left: 20rpx;
}
.nagps{
    position: relative;
}
.pad-fill55{
    padding-top: 35rpx;
}
.client-info .dingwei{
    height: 80rpx;
}
.returnindex{
    width: 76rpx;
    height: 76rpx;
    background:rgba(0, 0, 0, 0.7);
    top: 64%;
    right: 20rpx;
    border-radius:50rpx;
    position: fixed; 
    z-index:200;  
}    
.returnindexpic{
    width: 38rpx;
