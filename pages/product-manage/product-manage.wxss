/* pages/product-manage/product-manage.wxss */
.pro-list{
    padding-bottom: 40rpx;
}
.pro-box {
    background: #fff;
    padding: 25rpx 3%;
    position: relative;
    height: 170rpx;
    margin: 20rpx 25rpx 0 25rpx;
}

.pro-pic {
    width: 32%;
    display: inline-block;
    height: 170rpx;
}

.com-box {
    height: 110rpx;
    position: fixed;
    bottom: 0;
    width: 100%;
    background: #fff;
}

.pro-pic image {
    width: 100%;
    height: 100%;
}

.pro-name {
    width: 65%;
    margin-left: 3%;
    display: inline-block;
    vertical-align: top;
}

.pro-title {
    font-size: 34rpx;
    margin-bottom: 14rpx;
}

.pro-describe {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #888;
}

.client-img {
    position: absolute;
    width: 160rpx;
    height: 38rpx;
    bottom: 20rpx;
    right: 20rpx;
}

.client-img image {
    width: 39rpx;
    height: 40rpx;
    margin-left: 40rpx;
}

.pic-btn {
    text-align: center;
    line-height: 90rpx;
    background: #2ab7ff;
    color: #fff;
    margin-top: 40rpx;
    width: 96%;
    margin-left: 2%;
    border-radius: 8rpx;
    font-size: 32rpx;
    position: fixed;
    bottom: 10rpx;
}
.returnindex{
    width: 76rpx;
    height: 76rpx;
    background:rgba(0, 0, 0, 0.7);
    top: 64%;
    right: 20rpx;
    border-radius:50rpx;
    position: fixed;   
    z-index:200;
}    
.returnindexpic{
    width: 38rpx;
    height: 38rpx;
    /* margin-left:19rpx;
    margin-top:19rpx; */
    position: absolute;
    text-align: center;
    top:25%;
    left: 25%;
}