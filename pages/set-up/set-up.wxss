
.set-up {
  background-color: #ffffff;
}

.item {
  display: flex;
  align-items: center;
  margin: 0 30rpx;
  height: 90rpx;
  border-bottom: 0.5rpx solid #E5E5E5;
}

.label {
  font-size: 30rpx;
  color: #1A1A1A;
  margin-right: auto;
  min-width: 160rpx;
}

.content {
  flex: 1;
  display: flex;
  align-items: center;
}

.val {
  flex: 1;
  text-align: right;
  font-size: 26rpx;
  color: #666666;
}

.more {
  padding: 30rpx;
  margin-right: -30rpx;
}

.icon-more {
  width: 16rpx;
  height: 24rpx;
}

.head-item {
  height: 174rpx;
}

.head-portrait {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.btn {
  background-color: #ffffff;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, .05);
  display: flex;
  align-items: center;
  justify-content: center;
  width: 690rpx;
  height: 70rpx;
  margin: 60rpx auto;
  border-radius: 24rpx;
  font-size: 32rpx;
  color: #1A1A1A;
}