.my-title {
    background: #fff;
}

.my-title view {
    font-size: 34rpx;
    text-align: center;
    display: inline-block;
    width: 50%;
    line-height: 90rpx;
}

.active {
    color: #ff9801;
    border-bottom: 5rpx solid #ff9801;
}

.my-list {
    margin-top: 20rpx;
    background: #fff;
    padding: 5rpx 25rpx;
}

.my-left, .my-right {
    display: inline-block;
    width: 30%;
    font-size: 30rpx;
    line-height: 90rpx;
}

.my-right {
    text-align: right;
    width: 70%;
}

.string {
    height: 3rpx;
    background: #f0f0f0;
}

.my-btn {
    position: fixed;
    width: 100%;
    bottom: 0;
    line-height: 90rpx;
    color: #fff;
    background: #ff9801;
    text-align: center;
    font-size: 32rpx;
}

.my-btn view {
    display: inline-block;
    width: 50%;
}

.my-color {
    background: #999;
}
