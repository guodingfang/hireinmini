.company-row {
    background: #fff;
    margin: 0 20rpx;
    margin-top: 18rpx;
}

.comp-title {
    max-width: 420rpx;
    font-size: 32rpx;
    color: #555;
}

.comp-img {
    width: 180rpx;
    height: 180rpx;
    display: inline-block;
    padding-right: 3%;
    vertical-align: top;
}

.comp-img image {
    width: 100%;
    height: 100%;
}

.comp-content {
    padding: 20rpx;
}

.comp-right {
    display: inline-block;
    width: 440rpx;
    font-size: 27rpx;
    color: #888;
    line-height: 60rpx;
    height: 180rpx;
}

.comp-con {
    overflow: hidden;
}

.co-tap {
    margin-top: 10rpx;
}

.work-list {
    max-width: 448rpx;
}

.work {
    margin-right: 6rpx;
    padding: 0 8rpx;
    display: inline-block;
    text-align: center;
    border: 1rpx solid #fea201;
    border-radius: 6rpx;
    font-size: 26rpx;
    color: #fea201;
    line-height: 40rpx;
    min-width: 110rpx;
}

.comp-content .city {
    color: #ccc;
    max-width: 440rpx;
}

.mycompany {
    display:flex;
    align-items:center;
    justify-content:center;
    position: fixed;
    right: 26rpx;
    bottom: 160rpx;
    width: 100rpx;
    height: 100rpx;
    background: #000;
    border-radius: 50%;
    filter: alpha(Opacity=70);
    -moz-opacity: 0.7;
    opacity: 0.7;
}

.mycompany .name {
    display: inline-block;
    width: 60rpx;
    line-height: 34rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}
.returnindex{
    width: 76rpx;
    height: 76rpx;
    background:rgba(0, 0, 0, 0.7);
    top: 64%;
    right: 20rpx;
    border-radius:50rpx;
    position: fixed;
    z-index:200;   
}    
.returnindexpic{
    width: 38rpx;
    height: 38rpx;
    /* margin-left:19rpx;
    margin-top:19rpx; */
    position: absolute;
    text-align: center;
    top:25%;
    left: 25%;
}