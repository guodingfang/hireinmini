/**index.wxss**/
@import "../../common/lib/weui.wxss";
@import "../../common/css/publiclist.wxss";

page {
	background: #f0f0f0;
}

.dark {
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.7);
	z-index: 100;
}

.g-city {
	position: fixed;
	top: 96rpx;
	left: 63rpx;
	width: 50%;
	height: 14%;
}

.g-search {
	position: fixed;
	top: 97rpx;
	right: 89rpx;
	width: 67%;
	height: 32%;
}

.btn-know {
	width: 256rpx;
	height: 84rpx;
	background: #d43d3d;
	border-radius: 46rpx;
	text-align: center;
	line-height: 84rpx;
	color: #fff;
	font-family: "simhei";
	font-size: 34rpx;
	position: fixed;
	top: 50%;
	left: 50%;
	margin-left: -128rpx;
}

.index-header-search {
	display: flex;
	height: 100rpx;
	background: #2ab7ff;
	line-height: 100rpx;
}

.icon-search {
	position: absolute;
	top: 28rpx;
	left: 25rpx;
	width: 40rpx;
	height: 40rpx;
}

.search-input {
	padding-left: 80rpx;
	margin-top: 13rpx;
	width: 82%;
	height: 74rpx;
	color: #ccc;
	background: #fff;
	border-radius: 37rpx;
	line-height: 74rpx;
	font-size: 30rpx;
}

.index-search-city {
	padding-left: 20rpx;
	color: #fff;
	font-size: 30rpx;
	font-family: "simhei";
}

.index-city__label {
	display: inline-block;
	max-width: 130rpx;
}

.icon-bottom {
	position: absolute;
	top: 44rpx;
	padding-left: 6rpx;
	width: 27rpx;
	height: 14rpx;
}

.index-nav-list {
	margin-top: 20rpx;
	padding: 23rpx 20rpx 15rpx 20rpx;
	text-align: center;
	font-family: "simhei";
	background: #fff;
}

.icon-nav {
	width: 59%;
}

.index-nav-list .nav-name {
	margin-top: -5rpx;
	font-size: 30rpx;
}

.carousel-img {
	width: 100%;
	height: 150px;
}

.box2 {
	font-size: 25rpx;
	display: flex;
	align-items: center;
}

.focus-wrap {
	width: 71rpx;
	position: fixed;
	right: 22rpx;
	bottom: 85rpx;
	text-align: center;
	font-size: 26rpx;
	padding: 7rpx;
	line-height: 35rpx;
	z-index: 1000;
}

.focus-wrap .focus-item1 {
	background: rgba(0, 0, 0, .7);
	color: #fff;
	width: 80rpx;
	height: 80rpx;
	line-height: 80rpx;
	border-radius: 50%;
	position: relative;
	top: -18rpx;
}

.focus-wrap .focus-item2 {
	background: rgba(0, 0, 0, .7);
	color: #fff;
	width: 80rpx;
	height: 80rpx;
	line-height: 80rpx;
	border-radius: 50%;
}

contact-button {
	line-height: 106rpx;
	display: block;
}

.focus-wrap .focus-item:last-child {
	border-bottom: 0;
}

.focus-mask {
	width: 70%;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -35%;
	margin-top: -250rpx;
	background: #fff;
	z-index: 101;
	text-align: center;
	border-radius: 24rpx;
}

.focus-mask .focus-img image {
	width: 430rpx;
	height: 430rpx;
	padding-top: 36rpx;
}

.focus-follow {
	height: 50rpx;
	font-size: 30rpx;
	background: #fff;
	margin-top: -21rpx;
}

.line-css {
	height: 1rpx;
	background: #efefef;
	margin-top: 20rpx;
}

.focus-confirm {
	font-size: 30rpx;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	background: #fff;
	border-radius: 24rpx;
}

.btn {
	padding: 0;
	line-height: 52rpx;
	background: none;
}

.btn::after {
	border: none;
}

/* 租圈列表 :start */
.NewIssue {
	padding: 0 30rpx;
	margin-top: 20rpx;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 32rpx;
	border-bottom: 1rpx solid #f0f0f0;
	background: #fff;
}

.NewIssue .issue-img {
	padding-right: 20rpx;
	width: 38rpx;
	height: 38rpx;
	vertical-align: middle;
}

.content-list {
	margin: 0 26rpx;
}

.cont-wrap {
	background: #fff;
}

/* 租圈列表 :end */

/* 轮播图 */
.m-carousel {
	width: 750rpx;
	height: 300rpx;
	margin: auto;
}

.m-carousel image {
	border-radius: 24rpx;
}

.fixed-item {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, .7);
  color: #ffffff;
}

.fixed-item + .fixed-item {
	margin-top: 12rpx	;
}

.fixed-button {
  padding: 0;
  background-color: transparent;
  border: 0rpx solid transparent;
}

.fixed-item, .fixed-button {
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
}