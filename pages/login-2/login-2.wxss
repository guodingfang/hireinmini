Page {
  background-color: #ffffff;
}

.not-register,
.user-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 120rpx;
}

.logo,
.user-logo {
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
}

.user-name {
  color: #1A1A1A;
  font-size: 36rpx;
  margin-top: 26rpx;
}

.button {
  margin-top: 60rpx;
  width: 690rpx;
}

.phone-register {
  padding: 24rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.phone-register .title {
  color: #1A1A1A;
  font-size: 32rpx;
  margin-bottom: 12rpx;
}

.input-model {
  width: 100%;
  height: 100rpx;
  display: flex;
  background-color: #ffffff;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
}

.input-model .icon {
  width: 30rpx;
  height: 40rpx;
}

.input-model .label {
  margin: 0 20rpx;
  color: #808080;
  font-size: 28rpx;
}

.input-val {
  flex: 1;
  background-color: #F5F4F9;
  border-radius: 20rpx;
  height: 70rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.pl-input {
  color: #999999;
  font-size: 28rpx;
}

.code-btn {
  position: absolute;
  right: 32rpx;
  background-color: #ffffff;
  height: 66rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 12rpx;
  border-radius: 20rpx;
  color: #1A1A1A;
  font-size: 26rpx;
}