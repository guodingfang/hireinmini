<view class="model">
  <view
    wx:for="{{list}}"
    wx:key="id"
    class="item"
  >
    <image class="icon-consult" src="/images/public/icon-consult.png"></image>
    <view class="info">
      <text class="val single-row-overstep">{{item.content}}</text>
      <text class="tips">{{item.answernum}}个回答</text>
    </view>
  </view>
</view>