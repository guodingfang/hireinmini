@import "/common/lib/iconfont.wxss";
.model {
  display: flex;
  flex-direction: column;
  border-radius: 24rpx;
  overflow: hidden;
  margin-bottom: 24rpx;
}

.content-item {
  position: relative;
}

.content-item:not(:last-child):after {
  position: absolute;
  content: '';
  width: 690rpx;
  height: 1rpx;
  left: 0;
  right: 0;
  background-color: #E5E5E5;
  z-index: 1;
  margin: auto;
}