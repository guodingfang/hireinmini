<view class="card">
  <view class="header" bind:tap="onSkipCompany">
    <text class="title">租赁知识</text>
    <view class="more">
      <text>更多</text>
      <text class="iconfont icon-gengduo"></text>
    </view>
  </view>
  <view class="list">
    <view class="item" bind:tap="onSkipCompany">
      <image class="icon" src='/images/user/user-3.png'></image>
      <text class="label">学习中</text>
    </view>
    <view class="item" bind:tap="onSkipClient">
      <image class="icon" src='/images/user/user-5.png'></image>
      <text class="label">审核中</text>
    </view>
    <view class="item" bind:tap="onMemberManage">
      <image class="icon icon-menber" src='/images/user/user-1.png'>
        <text class="num" wx:if="{{info && info.auditcount}}">{{info.auditcount}}</text>
      </image>
      <text class="label">已发布</text>
    </view>
  </view>
</view>