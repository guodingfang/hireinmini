.status {
  margin: auto;
  width: 133rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #FFA12C;
  background-color: #FFEED9;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 12rpx;
}

.concern-users {
  max-width: 140rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.user-logos {
  position: relative;
  width: 74rpx;
  height: 34rpx;
  margin-right: 6rpx;
}

.logo {
  position: absolute;
  top: 0;
  width: 34rpx;
  height: 34rpx;
  border: 1rpx solid #F5F5F5;
  box-shadow: 0 0 10rpx rgba(4, 0, 230, 0.2);
  border-radius: 50%;
}

.num {
  color: #666666;
  font-size: 22rpx;
}