
.about {
  display: flex;
  flex-direction: column;
  width: 690rpx;
  background-color: #F5F4F9;
  margin: auto;
  padding: 20rpx;
  box-sizing: border-box;
  margin: 20rpx auto;
  border-radius: 20rpx;
}

.about-item {
  display: flex;
}

.about-item + .about-item {
  margin-top: 20rpx;
}

.label {
  min-width: 140rpx;
  color: #666666;
  font-size: 28rpx;
}

.val {
  color: #1A1A1A;
  font-size: 28rpx;
}